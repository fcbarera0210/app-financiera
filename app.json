{
  "expo": {
    "name": "app-financiera",
    "slug": "app-financiera",
    "version": "0.4.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.fcabrera0210.appfinanciera",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F4F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fcabrera0210.appfinanciera"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F0F4F7"
      },
      "package": "com.fcabrera0210.appfinanciera"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          },
          "ios": {
            "googleServicesFile": "./GoogleService-Info.plist"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2cb7f96e-74b5-4285-b40d-702258f0c985"
      }
    }
  }
}
